<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="484.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tools.gui.controller.HTMLEditorController">
    <children>
        <TextArea fx:id="mTAEditor" prefHeight="503.0" prefWidth="640.0" />
        <AnchorPane prefHeight="27.0" prefWidth="640.0">
            <children>
                <Button fx:id="mBTSave" layoutX="447.0" layoutY="1.0" mnemonicParsing="false" onAction="#onAction" text="保存" />
                <Button fx:id="mBTCancel" layoutX="503.0" layoutY="1.0" mnemonicParsing="false" onAction="#onAction" text="取消" />
                <TextField fx:id="mTFFindInput" layoutY="1.0" />
                <Button fx:id="mBTFind" layoutX="268.0" layoutY="1.0" mnemonicParsing="false" onAction="#onAction" text="查找" />
            <RadioButton fx:id="mRBUp" layoutX="165.0" layoutY="4.0" mnemonicParsing="false" text="向上" />
            <RadioButton fx:id="mRBDown" layoutX="216.0" layoutY="4.0" mnemonicParsing="false" text="向下" />
            </children>
        </AnchorPane>
    </children>
</VBox>
