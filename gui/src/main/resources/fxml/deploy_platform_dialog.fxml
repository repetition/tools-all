<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">

    <children>
        <Label fx:id="mLASelector" alignment="CENTER" text="部署机服务IP">
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
         <font>
            <Font size="23.0" />
         </font>
        </Label>
        <HBox alignment="CENTER" spacing="3">
            <children>
                <TextField fx:id="mTFAgentAddress" promptText="服务ip地址" text="10.10.10.214"/>
            </children>
        </HBox>
        <Button fx:id="mBTSwitch" text="切换">
            <VBox.margin>
                <Insets bottom="5.0" top="5" />
            </VBox.margin>
         <font>
            <Font size="14.0" />
         </font>
        </Button>
    </children>
</VBox>
